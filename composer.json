    }
  },
  "require": {
    "mi/guzzle-service-builder": "~1.0@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7"
  },
  "autoload": {
    "psr-4": {
      "Mi\\VideoManager\\SDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mi\\VideoManager\\SDK\\Tests\\": "tests/"
    }
