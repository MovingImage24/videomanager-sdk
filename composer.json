{
  "name": "mi/videomanager-sdk",
  "description": "SDK for the VideoManager REST API",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "sdk",
    "videomanager"
  ],
  "authors": [
    {
      "name": "Alexander Miehe",
      "email": "alexander.miehe@movingimage.com"
    }
  ],
  "minimum-stability": "beta",
  "autoload": {
    "psr-4": {
      "ScoreYa\\Cinderella\\SDK\\": "src/"
